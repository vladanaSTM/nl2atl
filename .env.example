# Example environment variables for nl2atl
# Copy this to `.env` and fill in values as needed.

# Azure OpenAI / Azure inference settings
AZURE_API_KEY=
AZURE_INFER_ENDPOINT=
# Optional: name of the deployment/model to use for inference
AZURE_INFER_MODEL=
AZURE_API_VERSION=
AZURE_USE_CACHE=true
AZURE_VERIFY_SSL=false

# HuggingFace token (for private models)
HUGGINGFACE_TOKEN=

# Optional: Weights & Biases API key if you use W&B
WANDB_API_KEY=
